# BE/src/main/resources/application.properties.example

# 데이터베이스 설정 (각자 자신의 로컬 DB 정보로 채워주세요)
spring.datasource.url=jdbc:mysql://localhost:3306/mydb
spring.datasource.username=여기에_DB아이디
spring.datasource.password=여기에_DB비밀번호
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.database-platform=org.hibernate.dialect.MySQLDialect

# Google Gemini API 설정 (각자 자신의 API 키를 발급받아 채워주세요)
gemini.api-key=여기에_Gemini_API_키
gemini.project-id=gemini-api-project-id
gemini.location=asia-northeast3